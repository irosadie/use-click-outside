{
	"compilerOptions": {
		"target": "es6",
		"module": "esnext",
		"jsx": "react-jsx",
		"declaration": true,
		"declarationDir": "dist",
		"emitDeclarationOnly": false,
		"outDir": "dist",
		"strict": true,
		"esModuleInterop": true,
		"moduleResolution": "node",
		"skipLibCheck": true
	},
  	"include": ["src"]
}